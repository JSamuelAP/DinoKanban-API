{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": "off",
    "object-curly-newline": "off",
    "consistent-return": "off",
    "class-methods-use-this": "off",
    "no-param-reassign": "off",
    "func-names": "off"
  }
}
